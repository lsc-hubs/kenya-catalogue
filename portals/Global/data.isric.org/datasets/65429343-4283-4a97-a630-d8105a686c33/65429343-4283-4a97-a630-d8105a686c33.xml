          <gmd:citation>
            <gmd:CI_Citation>
              <gmd:title>
                <gco:CharacterString>Soil Dataset for Pedotransfer Function Development (IGBP-DIS)</gco:CharacterString>
              </gmd:title>
              <gmd:date>
                <gmd:CI_Date>
                  <gmd:date>
                    <gco:Date>1996-10-01</gco:Date>
                  </gmd:date>
                  <gmd:dateType>
